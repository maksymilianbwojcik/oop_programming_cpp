cmake_minimum_required(VERSION 3.14)
project(Desktop)

set(CMAKE_CXX_STANDARD 17)
set(Desktop gtest/gtest.h)
include_directories(~/googletest/googletest/include/)

target_link_directories(Desktop PUBLIC ~/googletest/build/lib/)
target_link_libraries(Desktop PUBLIC libgtest_main.a pthread)
target_link_libraries(Desktop PUBLIC libgtest.a pthread)
add_executable(Desktopexec.cxx main.cpp)
set_target_properties(Desktop PROPERTIES LINKER_LANGUAGE C)